{
    // Use IntelliSense to learn about possible attributes.
    // Hover to view descriptions of existing attributes.
    // For more information, visit: https://go.microsoft.com/fwlink/?linkid=830387
    "version": "0.2.0",
    "configurations": [
        {
            "name": "Prefect: Current File",
            "type": "debugpy",
            "request": "launch",
            "program": "${file}",
            "console": "integratedTerminal",
            "env": {
                "AWS_PROFILE": "sandbox",
                // allows imports from dags., plugins., etc. from prefect during debugging,
                // which mirrors the value of the variable when running a DAG via the prefect server
                "PYTHONPATH": "${workspaceFolder}/prefect"
            },
        },
        {
            "name": "Airflow: Current File",
            "type": "debugpy",
            "request": "launch",
            "program": "${file}",
            "console": "integratedTerminal",
            "env": {
                "AWS_PROFILE": "sandbox",
                "PYTHONPATH": "${workspaceFolder}/prefect"
            },
            "args": ["run"],
            "justMyCode": false
        }
    ]
}